
# Only change last year. DO NOT use generic matching '^Copyright' because
# it will change other people's Copyright.

find -L . \( -name "*.h" -or -name "*.c" -or -name "*.hh" -or -name "*.cpp" -or -name "*.mk" -or -name "*.s" -or -name "*.ldf" -or -name "*.sh" \) -type f | xargs sed -i 's/cadena_inexistente/cadena_inexistente/g'

