x4 idx = (x4 + (b ^ x5))
x3 idx = (x3 + (b ^ (x4 + (b ^ x5))))
x2 idx = (x2 + (b ^ (x3 + (b ^ (x4 + (b ^ x5))))))
x1 idx = (x1 + (b ^ (x2 + (b ^ (x3 + (b ^ (x4 + (b ^ x5))))))))

